{
    "name": "ori-mcc",
    "version": "1.0.0",
    "types": "./build/Index.d.ts",
    "exports": {
        ".": {
            "import": "./build/Index.js",
            "require": "./build/Index.js"
        }
    },
    "description": "Ori-MCC: A comprehensive TypeScript library for Minecraft launcher functionality with mod loader support, authentication, and file management.",
    "scripts": {
        "dev": "rimraf ./build && tsc -w",
        "build": "rimraf ./build && tsc"
    },
    "files": [
        "assets/**",
        "build/**",
        "LICENSE",
        "README.md"
    ],
    "keywords": [
        "minecraft",
        "launcher",
        "ori-mcc",
        "minecraft-launcher",
        "typescript",
        "nodejs",
        "game-launcher",
        "minecraft-core",
        "mod-loader",
        "forge",
        "fabric",
        "quilt",
        "neoforge",
        "minecraft-authentication",
        "microsoft-auth",
        "mojang",
        "minecraft-java",
        "file-downloader",
        "minecraft-assets",
        "minecraft-libraries",
        "game-management",
        "minecraft-versions"
    ],
    "author": "Cosmic-fi",
    "license": "CCANC",
    "dependencies": {
        "adm-zip": "^0.5.9",
        "node-fetch": "^2.6.9",
        "prompt": "^1.2.1",
        "tslib": "^2.4.1"
    },
    "devDependencies": {
        "@types/adm-zip": "^0.5.2",
        "@types/node": "^18.11.13",
        "@types/node-fetch": "^2.6.2",
        "rimraf": "^3.0.2",
        "typescript": "^4.9.4"
    },
    "bugs": {
        "url": "https://github.com/cosmic-fi/ori-mcc/issues"
    },
    "homepage": "https://github.com/cosmic-fi/ori-mcc#readme",
    "repository": {
        "type": "git",
        "url": "git@github.com:cosmic-fi/ori-mcc.git"
    }
}
